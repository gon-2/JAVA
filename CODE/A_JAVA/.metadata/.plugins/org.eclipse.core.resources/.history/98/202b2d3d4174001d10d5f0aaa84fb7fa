package com.mc.g_override_overloadingA_override;

public class Run {

	public static void main(String[] args) {
		
		//반지름이 5인 원의 넓이를 구하시오
		Circle cir = new Circle(5);
		System.out.println("원의 넓이: " + cir);
		
		// 너비가 4 높이가 7인 직사각형의 넓이를 구하시오.
		Rectangle rec = new Rectangle(4,7);
		System.out.println("직사각형의 넓이: " + rec);
	}
}
